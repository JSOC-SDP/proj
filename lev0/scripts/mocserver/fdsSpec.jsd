Keyword:	OBS_DATE_unit, string, constant, record, days, %s, "none", "ts_eq unit"
Keyword:	DATA_FORMAT, string, variable, record, "None", %s, "None", "Format of data file contained in segment"
Keyword:	FILE_VERSION, int, variable, record, 0, %d, "None", "Version of the data file"

# ************************** Data Segments ***************************
Data:		FILENAME, variable, string, 1, 1, "none", generic, "Product file name."
